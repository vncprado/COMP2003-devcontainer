#! /usr/local/bin/vvp
:ivl_version "11.0 (stable)" "(v11_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x55f4810a0da0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55f4810a0f30 .scope module, "main" "main" 3 4;
 .timescale -9 -9;
v0x55f4810ee900_0 .var/i "all_inputs", 31 0;
v0x55f4810ee9e0_0 .var "expected", 0 0;
v0x55f4810eeaa0_0 .var/i "i", 31 0;
v0x55f4810eeb90_0 .net "v", 3 0, L_0x55f4810eed70;  1 drivers
v0x55f4810eec80_0 .net "y", 0 0, v0x55f4810ee800_0;  1 drivers
L_0x55f4810eed70 .part v0x55f4810ee900_0, 0, 4;
S_0x55f4810c9800 .scope module, "dut" "mod3_12" 3 10, 4 1 0, S_0x55f4810a0f30;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "y";
    .port_info 1 /INPUT 4 "v";
v0x55f4810c99e0_0 .var/i "m", 31 0;
v0x55f4810ee720_0 .net "v", 3 0, L_0x55f4810eed70;  alias, 1 drivers
v0x55f4810ee800_0 .var "y", 0 0;
E_0x55f4810da740 .event edge, v0x55f4810ee720_0;
    .scope S_0x55f4810c9800;
T_0 ;
Ewait_0 .event/or E_0x55f4810da740, E_0x0;
    %wait Ewait_0;
    %load/vec4 v0x55f4810ee720_0;
    %pad/u 32;
    %pushi/vec4 3, 0, 32;
    %mod;
    %store/vec4 v0x55f4810c99e0_0, 0, 32;
    %load/vec4 v0x55f4810c99e0_0;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x55f4810c99e0_0;
    %pushi/vec4 2, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %or;
    %store/vec4 v0x55f4810ee800_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55f4810a0f30;
T_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55f4810eeaa0_0, 0, 32;
    %pushi/vec4 16, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %load/vec4 v0x55f4810eeaa0_0;
    %store/vec4 v0x55f4810ee900_0, 0, 32;
    %delay 10, 0;
    %load/vec4 v0x55f4810eeb90_0;
    %pad/u 32;
    %pushi/vec4 3, 0, 32;
    %mod;
    %pushi/vec4 0, 0, 32;
    %cmp/ne;
    %flag_get/vec4 4;
    %store/vec4 v0x55f4810ee9e0_0, 0, 1;
    %vpi_call/w 3 20 "$display", "t=%03d  v=%04b y=%b expected=%b", $time, v0x55f4810eeb90_0, v0x55f4810eec80_0, v0x55f4810ee9e0_0 {0 0 0};
    %load/vec4 v0x55f4810ee9e0_0;
    %load/vec4 v0x55f4810eec80_0;
    %cmp/ne;
    %jmp/0xz  T_1.2, 4;
    %vpi_call/w 3 23 "$display", "expected does not match" {0 0 0};
T_1.2 ;
    %load/vec4 v0x55f4810eeaa0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55f4810eeaa0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %vpi_call/w 3 28 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
